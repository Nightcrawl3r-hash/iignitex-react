
{
    "name": "iignitex",
    "version": 2,
    "builds": [
      {
        "src": "app.jsx",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/iignitex-react/(.*)", "dest": "/iignitex-react/$1" },
      { "handle": "filesystem" },
      { "src": "/.*", "status": 404, "dest": "/404.html" }
    ]
  }
  